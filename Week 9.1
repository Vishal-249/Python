def productDigits(n):
    # Convert the number to a string to easily access digits by their positions
    number_str = str(n)
    
    # Initialize variables for product of digits at even places and sum of digits at odd places
    product_even = 1
    sum_odd = 0
    
    # Iterate over the digits and their positions
    for i, digit in enumerate(number_str):
        digit = int(digit)
        if (i + 1) % 2 == 0:
            # i + 1 is the position; if it's even, we multiply the product
            product_even *= digit
        else:
            # If it's odd, we add to the sum
            sum_odd += digit
    
    # Check if the product of even-place digits is divisible by the sum of odd-place digits
    if sum_odd == 0:  # To handle cases where sum_odd is zero to avoid division by zero
        return False
    return product_even % sum_odd == 0

# Input from stdin
try:
    n = int(input())

# Output the result
    if productDigits(n):
        print("TRUE")
    else:
        print("FALSE")
except EOFError:
    pass
